<h1>Students</h1>
 

<table class = "table table-bordered table-striped">
<thead>.
   <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Gender</th>
       
   </tr>
</thead>

<tbody>
 <% @students.each do |student| %>
      
      <tr>
      <td><%= student.name %></td>
      <td><%= student.age %></td>
      <td><%= student.gender%></td>
      
   <% end %>
</tbody>

</table>


